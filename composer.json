{
    "name": "numanrki/whmcs-license",
    "description": "A Laravel package for WHMCS license verification",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Numan RKI",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/validation": "^10.0|^11.0",
        "illuminate/cache": "^10.0|^11.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Numanrki\\WhmcsLicense\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Numanrki\\WhmcsLicense\\WhmcsLicenseServiceProvider"
            ],
            "aliases": {
                "WhmcsLicense": "Numanrki\\WhmcsLicense\\Facades\\WhmcsLicense"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/pest"
    }
}